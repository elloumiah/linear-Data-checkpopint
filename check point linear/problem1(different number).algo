ALGORITHM problem1 ;
VAR
tab1,tab2 : ARRAY_OF INTEGER[]; 
i : INTEGER;
n,m : integer ;
sum : integer :=0;
BEGIN
REPEAT //table 1
    write(enter table 1 length);
    read(n);
UNTIL (n>0)
FOR i FROM 0 TO n-1 STEP 1  DO   
    REPEAT
    Write(tab[i]) ;
    read (tab[i]);
    UNTIL (Tab[i]=='integer')
END_FOR

REPEAT //table 2 
    write(enter table 2 length);
    read(n);
UNTIL (m>0 and m<n)
FOR i FROM 0 TO m-1 STEP 1  DO   
    REPEAT
    Write(tab[i]) ;
    read (tab[i]);
    UNTIL (Tab[i]>0)
END_FOR
//sum function with distinct number
FOR i FROM 0 TO tab1.length STEP 1  DO //somme function 
    FOR j FROM 0 TO tab2.length STEP 1  DO
        if (tab1[i]<>tab2[j])
        sum+=tab1[i];
    END_FOR
END_FOR
write(sum);
END
